{% extends "base.html.twig" %}

{% block title %}{{ titre_doc }}{% endblock %}

{% block content %}
  <h1>{{ titre_page }}</h1>

  <div class="container">
    {% for item in articles | sort %}
      <div class="articles">
        <a href="article?{{ item.slug }}"> {{ item.titre }}</a>
      </div>
    {% endfor %}
  </div>
{% endblock %}
